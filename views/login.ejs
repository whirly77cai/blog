<%- include header %>
<br>
<h2>Login</h2>
<br>
<form class="form-horizontal" method="POST">
  <div class="form-group">
    <label for="inputText1" class="col-sm-3 control-label">Username</label>
    <div class="col-sm-3">
    	<input type="text" class="form-control" name="username_l" id="inputText1" placeholder="Username" autofocus="true">
    </div>
  </div>
  <div class="form-group">
    <label for="inputPassword1" class="col-sm-3 control-label">Password</label>
    <div class="col-sm-3">
      <input type="password" class="form-control" name="password_l" id="inputPassword1" placeholder="Password">
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-3 col-sm-3">
      <div class="checkbox">
        <label>
          <input type="checkbox"> Remember me
        </label>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-3 col-sm-9">
      <button type="submit" class="btn btn-default">Sign in</button>
    </div>
  </div>
</form>
<%- include footer %>